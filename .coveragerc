[run]
source = .
omit = 
    tests/*
    .venv/*
    venv/*
    */site-packages/*
    conftest.py
    setup.py
    */__pycache__/*
    */migrations/*
    pyscript
    shibokensupport/*
    signature_bootstrap.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
